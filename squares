			var arr = [];


  					for(var i=0; i < sqn; i++){

						arr.push([]);

						arr[i].push( new Array(sqn));

						for(var j=0; j < sqn; j++){

						arr[i][j] = 0;




						};
							

							 // console.log(arr[i])
					};


					arr[0][1] = 1;
					arr[2][3] = 2;
					arr[1][2] = 3;

					for(var x = 0; x < arr.length; x++){
						for (var y = 0; y < arr[i].length; y++){
							console.log(arr[i])
						}
					}

					
		
		}

